<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.txx.dao.StudentDao">
    <select id="getStudentById" parameterType="java.lang.Integer" resultType="com.txx.entity.pojo.Student">
        SELECT  * FROM t_student WHERE  id=#{id}
    </select>
</mapper>